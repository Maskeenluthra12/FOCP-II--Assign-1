#include <iostream>
#include <iomanip>
using namespace std;

int main() 
{
    int numbers[7];

  cout << "Enter 7 elements: " << endl;
  for (int i = 0; i < 7; ++i) {
    cin >> numbers[i];
  }
cout << "The array elements are: ";
for (int n = 0; n < 7; ++n)
{
    cout << numbers[n] << "  ";
  }
    return 0;
}
