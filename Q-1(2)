#include <iostream>
#include <iomanip>
using namespace std;
int main() {
int n,count;
cout<<"enter a number:"<<endl;
cin>>n;
for(int i=1;i<=n;i++)
{
    if(n%i==0)
    {
        count++;
    }
    
}
if(count==2)
cout<<"it is a prime number"<<endl;
else
cout<<"not a prime number";
cout << "Factors of " << n << " are: ";  
  int i;
    for(i=1; i <= n; ++i) 
    {
        if(n % i == 0)
            cout << i << " ";
    }

    
    return 0;
}
